{
  "env": {
    "browser": true,
    "es6": true,
    "jquery": true
  },
  "extends": "airbnb",
  "plugins": [
    "filenames"
  ],
  "rules": {
    "filenames/match-regex": [2, "^[a-z0-9_]+(.js)?$"]
  },
  "globals": {
    "_": false,
    "beforeEach": false,
    "d3": false,
    "define": false,
    "describe": false,
    "expect": false,
    "fixture": false,
    "gl": false,
    "it": false,
    "Mousetrap": false,
    "spyOn": false,
    "spyOnEvent": false,
    "Turbolinks": false
  }
}
